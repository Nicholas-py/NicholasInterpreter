
ok=input(":").lower()
print('Calculating')
if "-" in ok:
	Split=ok.split("-")
	print(float(Split[0])-float(Split[1]))
if "+" in ok:
	Split=ok.split("+")
	print(float(Split[0])+float(Split[1]))
if "*" in ok:
	Split=ok.split("*")
	print(float(Split[0])*float(Split[1]))
if "/" in ok:
	Split=ok.split("/")
	print(float(Split[0])/float(Split[1]))


